<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ydc.basepack.mapper.SkuMapper">


    <select id="querySkuBySpuId" resultType="com.ydc.basepack.model.dto.SkuDTO">
        select
        *
        from
        tb_sku
        <where>
            <if test="id!=null">
                and spu_id=#{id}
            </if>
        </where>
    </select>


</mapper>
