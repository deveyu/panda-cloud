<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ydc.basepack.mapper.SpecGroupMapper">


    <select id="pageByQuery" resultType="com.ydc.basepack.model.entity.SpecGroup">
        select
        *
        from
        tb_spec_group
        <where>
            <if test="cid!=null ">
                and  cid=#{cid}
            </if>
            <if test="name!=null and name!=''">
                and  name=#{name}
            </if>
        </where>

    </select>

</mapper>
